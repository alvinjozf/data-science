arr=[]
n=int(input("enter the no of element:"))
for i in range (n):
    arr.append(int(input(f"enter the ele {i+1}:")))
print(f"Array: {arr}")
for i in  range (n):
    for j in  range (i+1,n):
         if arr[i]>arr[j]:
            temp=arr[i]
            arr[i]=arr[j]
            arr[j]=temp
print("sorted array:",arr)
